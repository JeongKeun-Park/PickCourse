<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.pickcourse.mapper.MemberMapper2">

    <select id="getMemberFileInfo" resultType="memberFileDTO">
        SELECT ID, MEMBER_FILE_PATH, MEMBER_FILE_NAME
        FROM TBL_MEMBER
        WHERE ID = #{id}
    </select>

</mapper>